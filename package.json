{
	"name": "extention-for-taro",
	"displayName": "extention for taro",
	"publisher": "wangzk",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.cratePage"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.cratePage",
				"title": "Crate Taro Page"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.cratePage",
					"when": "explorerResourceIsFolder",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"babel-generator": "^6.26.1",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"babel-traverse": "^6.26.0",
		"babylon": "^6.18.0",
		"escodegen": "^1.11.1"
	}
}
